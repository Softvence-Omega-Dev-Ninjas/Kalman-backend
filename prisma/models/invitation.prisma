model Invitation {
    id String @id @default(uuid())
    jobId String
    job Jobs @relation(fields: [jobId], references: [id])

    tradesManId String
    tradesMan TradesMan @relation(fields: [tradesManId],references: [id])

    messgae String
    
    createdAt       DateTime @default(now())
    updatedAt       DateTime @updatedAt
}