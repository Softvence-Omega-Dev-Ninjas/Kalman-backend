model Invitation {
    id String @id @default(uuid())
    title String?
    tradesManId String
    tradesMan TradesMan @relation(fields: [tradesManId],references: [id],onDelete: Cascade)
    date DateTime?
    time_slot String?
    messgae String
    userId String
    user User @relation(fields: [userId],references: [id])
    location String
    jobId String
    job Jobs @relation(fields: [jobId], references: [id],onDelete: Cascade)
    createdAt       DateTime @default(now())
    updatedAt       DateTime @updatedAt
}
