model Review {
  id         String   @id @default(uuid())
  rating     Float
  text       String
  customerId   String
  customer     User     @relation(fields: [customerId], references: [id])
  tradesManId String
  tradesMan   TradesMan     @relation( fields: [tradesManId], references: [id])
  createdAt  DateTime @default(now())
  updatedAt  DateTime @updatedAt
}
