model Review {
  id         String   @id @default(uuid())
  rating     Int
  text       String
  senderId   String
  sender     User     @relation(fields: [senderId], references: [id])
  receiverId String
  receiver   TradesMan     @relation( fields: [receiverId], references: [id])
  createdAt  DateTime @default(now())
  updatedAt  DateTime @updatedAt
}
